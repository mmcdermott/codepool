        <%= link_to image_tag("codepoollogo-sm.png", :id=>"header_image"), root_url %>

		
      
       
        <form class="pull-left" action="" id="header_search">
        <input type="text" id="search" placeholder="Search">
        </form>
        <ul class="nav" id="header_nav">
